{
  "manifest_version": 3,
  "name": "Web Technology Fingerprinter",
  "version": "1.0",
  "description": "Detect CMS, JS frameworks, analytics, and other tech on any website.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
